{
  "name": "aurea",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:check:staged": "node tools/check-prettier-staged.cjs",
    "encoding:check:staged": "node tools/check-encoding-staged.cjs",
    "commitmsg:check": "node tools/validate-commit-msg.cjs",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e:install": "npm install -D @playwright/test && npx playwright install chromium",
    "test:e2e:prepare": "node tools/e2e/prepare-test-db.mjs",
    "test:e2e": "node tools/e2e/run-playwright-e2e.mjs",
    "test:e2e:headed": "node tools/e2e/run-playwright-e2e.mjs --headed",
    "version:bump": "node scripts/bump-version.cjs",
    "precommit:check": "npm run format && npm run lint:fix && npm run format:check:staged && npm run encoding:check:staged && npm run test:run && npm run build",
    "hooks:install": "node tools/setup-githooks.cjs",
    "prepare": "npm run hooks:install"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@react-google-maps/api": "^2.20.8",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-table": "^8.11.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^5.3.4",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^5.5.6",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.33",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
