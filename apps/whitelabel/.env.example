# Supabase Configuration
# Get these values from your Supabase project settings
# Project Settings > API

VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Google Maps API
# Get your API key from https://console.cloud.google.com
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# E2E database automation
APP_ENV=dev
WHITELABEL_DB_URL=postgresql://postgres:<password>@db.<project-ref>.supabase.co:5432/postgres
WHITELABEL_SUPABASE_URL=https://<project-ref>.supabase.co
WHITELABEL_SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
WHITELABEL_E2E_ADMIN_EMAIL=e2e.admin@whitelabel.local
WHITELABEL_E2E_ADMIN_PASSWORD=WhiteLabelE2E!123
TEST_USER_EMAIL=e2e.admin@whitelabel.local
TEST_USER_PASSWORD=WhiteLabelE2E!123

